{
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "src/app.test.ts",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "modulePathIgnorePatterns": ["<rootDir>/dist/"],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -12
      }
    },
    "coveragePathIgnorePatterns": [
      
    ],
    "collectCoverageFrom": ["src/**/*.ts"],
    "coverageReporters": [
      "json",
      "html"
    ]
  }